<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.entity.PtUser" table="ptUser">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="name" column="name"></property>
		<property name="job" column="job"></property>
		<property name="startdata" column="startdata"></property>
		<property name="enddata" column="enddata"></property>
		<property name="selery" column="selery"></property>
		<property name="addr" column="addr"></property>
		<property name="test" column="test"></property>
		<property name="phone" column="phone"></property>
		<property name="userid" column="userid"></property>
		<property name="state" column="state"></property>
		<property name="remark" column="remark"></property>
		<property name="addtime" column="addtime"></property>
		<property name="isdelete" column="isdelete"></property>
	</class>
</hibernate-mapping>